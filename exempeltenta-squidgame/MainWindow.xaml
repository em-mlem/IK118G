<Window x:Class="exempeltenta_squidgame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:exempeltenta_squidgame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtNumbers" HorizontalAlignment="Left" Height="17" Margin="18,31,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="52"/>
        <Button x:Name="btnchknumbers" Content="Nummertest" HorizontalAlignment="Left" Margin="18,53,0,0" VerticalAlignment="Top" Click="btnchknumbers_Click"/>
        <Button x:Name="btnmagicnumber" Content="Magic Number!" HorizontalAlignment="Left" Margin="18,78,0,0" VerticalAlignment="Top" Click="btnmagicnumber_Click"/>
        <Button x:Name="btnguards" Content="Vakter" HorizontalAlignment="Left" Margin="18,103,0,0" VerticalAlignment="Top" Click="btnguards_Click"/>

    </Grid>
</Window>
